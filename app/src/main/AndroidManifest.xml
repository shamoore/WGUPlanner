<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smoo182.wguplanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activities.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activities.TermListActivity" />
        <activity android:name=".view.activities.AssessmentListActivity" />
        <activity android:name=".view.activities.CourseListActivity" />
        <activity android:name=".view.activities.MentorListActivity" />
        <activity android:name=".view.activities.TermDetailActivity" />
        <activity android:name=".view.activities.AssessmentDetailActivity"></activity>
    </application>

</manifest>